&run_pars
  cvsid='$Id: run.in,v 1.1 2014/04/24 15:15:40 wlyra Exp $',
  ip=14,
 !nt=10000000, it1=50, isave=1000, itorder=3
  nt=10, it1=1, isave=1000, itorder=3
  cdt=0.4,
  cdtv=0.4,
  cdtv3=0.4
  dtmin=1e-6,
  dsnap=6.2831
  dvid=0.062831
  tmax=1727.88
  slice_position='d'
  wborder_int=0.025
  wborder_ext=0.25
  max_walltime=42750
  lwrite_dvar=T
  crash_file_dtmin_factor=10.
!
  fraction_tborder=0.16
/
&eos_run_pars
/
&hydro_run_pars
