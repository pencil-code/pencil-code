/
&density_run_pars
  idiff='hyper3-sph' , 'shock'
  diffrho_hyper3=3e-4 , diffrho_shock=10.
  borderlnrho='initial-condition'
  density_floor=2e-6
/
&grav_run_pars
/
&viscosity_run_pars
  ivisc='hyper3-sph' , 'nu-shock'
  nu_hyper3=3e-4 , nu_shock=10.
/
&shock_run_pars
/
&magnetic_run_pars
  iresistivity='hyper3-sph' 'shock' ,'xdep'
  eta_hyper3=3e-4 , eta_shock=10.
  xdep_profile='two-step'
  eta=1e-3       !resistivity that quenches the MRI
                  !eta = 4*pi/sqrt(3) * 1/Re * cs2/Omega * 1/beta
  eta_jump=1.    !for this too. Resisitive boundary.
  two_step_factor=0.
!
  eta_x0=1.05
  eta_x1=4.65
/
