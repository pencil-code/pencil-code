!                           -*-f90-*-     (for Emacs)
!
!  Run parameters!
!
&run_pars
  cvsid='$Id: run.in,v 1.1 2012-10-04 16:04:16 wlyra Exp $',
  ip=14,
!  nt=1000000, it1=250, isave=5000, itorder=3, ialive=0
  nt=50, it1=5, isave=5000, itorder=3, ialive=0
  cdt=0.4, 
  cdtv=0.4, 
  cdtv3=0.1
  dtmin=1e-6
  tmax=6283.0
  wborder_int=0.1
  wborder_ext=0.1
!
  slice_position='m'
  crash_file_dtmin_factor=2
  lwrite_dvar=T
!
  lpencil_check=F
  lpencil_check_small=F
!
  max_walltime=85500
  tborder=0.1
!
  dsnap=6.28319
  dvid=0.1
  loutput_varn_at_exact_tsnap=T
  lread_aux=T
/
&eos_run_pars
/
&hydro_run_pars
