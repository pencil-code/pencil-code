!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Runtime parameters for harmonic oscillator
!
&run_pars
  cvsid="76d6d658f"
  lpencil_check_small=F, isave=500000, itorder=3
  nt=10001, it1=2000, d2davg=2000., tmax=-1e-7, dtmin=1e-20
